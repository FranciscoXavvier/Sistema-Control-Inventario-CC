            <Label layoutX="203.0" layoutY="12.0" text="Consultar Programas" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="btnAsignar" layoutX="372.0" layoutY="81.0" mnemonicParsing="false" onAction="#clicAsignarProgramas" styleClass="boton_registrar" stylesheets="@../estilos/estilos.css" text="Asignar Programas" />
