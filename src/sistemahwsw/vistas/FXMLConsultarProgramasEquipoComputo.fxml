          <TableColumn fx:id="colNombre" prefWidth="150.0" text="Nombre de aplicación" />
          <TableColumn fx:id="colEdicion" prefWidth="150.0" text="Edición" />
            <TableColumn fx:id="colOpciones" prefWidth="150.0" text="Opciones" />
